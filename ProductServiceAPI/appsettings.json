{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=VOLKAN\\VOLKAN;Database=KayraExport3_Products;Trusted_Connection=true; TrustServerCertificate=True;",
    "Redis": "localhost:6379"
  },
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyHereMakeItLongEnoughForSecurity123456789",
    "Issuer": "KayraExport3.Auth",
    "Audience": "KayraExport3.API"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/product-service-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "CacheSettings": {
    "DefaultExpirationMinutes": 5,
    "ProductListExpirationMinutes": 10,
    "ProductDetailExpirationMinutes": 30
  }
}
