{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "LogConnection": "Server=VOLKAN\\VOLKAN;Database=KayraExport3_Logs;Trusted_Connection=true; TrustServerCertificate=True;"
  },
  "JwtSettings": {
    "SecretKey": "YourSuperSecretKeyHereMakeItLongEnoughForSecurity123456789",
    "Issuer": "KayraExport3.Auth",
    "Audience": "KayraExport3.API"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-service-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 31
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "Elasticsearch": {
    "Url": "http://localhost:9200",
    "DefaultIndex": "kayraexport3-logs",
    "Username": "",
    "Password": ""
  }
}
